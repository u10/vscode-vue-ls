{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Extension",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}/client",
				"--enable-proposed-api=vaniship.vue-ls"
			],
			"stopOnEntry": false,
			"sourceMaps": true,
			"outFiles": ["${workspaceFolder}/client/out/**/*.js"]
		}
	]
}
